<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<StackPane fx:id="mainStackpane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="550.0">
         <children>
         	<Label fx:id="namelab" layoutX="100.0" layoutY="40.0" mnemonicParsing="false"  prefHeight="35.0" prefWidth="50.0" stylesheets="@style.css" text="Name:" />
         	<Label fx:id="vornamelab" layoutX="100.0" layoutY="130.0" mnemonicParsing="false"  prefHeight="35.0" prefWidth="60.0" stylesheets="@style.css" text="Vorname:" />
         	<Label fx:id="emaillab" layoutX="100.0" layoutY="220.0" mnemonicParsing="false"  prefHeight="35.0" prefWidth="50.0" stylesheets="@style.css" text="Email:" />
         	<Label fx:id="matrikelnummerlab" layoutX="100.0" layoutY="310.0" mnemonicParsing="false"  prefHeight="35.0" prefWidth="100.0" stylesheets="@style.css" text="Matrikelnummer:" />
         	
         	<TextField fx:id="nametxt" layoutX="200.0" layoutY="40.0"   prefHeight="35.0" prefWidth="200.0" stylesheets="@style.css"  />
         	<TextField fx:id="vornametxt" layoutX="200.0" layoutY="130.0"   prefHeight="35.0" prefWidth="200.0" stylesheets="@style.css"  />
         	<TextField fx:id="emailtxt" layoutX="200.0" layoutY="220.0"   prefHeight="35.0" prefWidth="200.0" stylesheets="@style.css"  />
         	<TextField fx:id="matrikelnummertxt" layoutX="200.0" layoutY="310.0"   prefHeight="35.0" prefWidth="200.0" stylesheets="@style.css"  />
         	
         	<Button fx:id="speichernbtn" layoutX="100.0" layoutY="350" mnemonicParsing="false"  prefHeight="35.0" prefWidth="300.0" stylesheets="@style.css" text="Speichern" onAction="#Speichern" />
         	<Button fx:id="abbrechenbtn" layoutX="100.0" layoutY="400"  mnemonicParsing="false"  prefHeight="35.0" prefWidth="300.0" stylesheets="@style.css" text="Abbrechen" onAction="#Abbrechen" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
